				"spans": [
					{
						"offset": 1111,
						"length": 197
					}
				],
				"content": "SECURE EMAIL Secure email is required for all email communications from FDA when confidential information (e.g., trade secrets, manufacturing, or patient information) is included in the message. To"
			},
